{
  "name": "atlas-app",
  "version": "0.2.0",
  "author": "FixMyCity <hello@fixmycity.de>",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "start": "blitz start",
    "dev": "blitz dev --hostname 127.0.0.1 --port 5173",
    "predev": "# TODO CHECK/START DOCKER IN ATLAS-GEO # if [[ $(docker inspect -f '{{.State.Running}}' atlas-db) = \"true\" ]]; then echo Docker: Skipping â€“ Container already running; else docker compose up --detach; fi",
    "build": "blitz build",
    "studio": "blitz prisma studio",
    "openTypes": "code \"node_modules/.prisma/client/index.d.ts\"",
    "migrate": "blitz prisma migrate dev",
    "migrate:create": "blitz prisma migrate dev --create-only && npm run migrate:create:openMigration",
    "migrate:create:openMigration": "code $(find ./db/migrations -type f -name \"migration.sql\" -cmin -1 | head -n 1)",
    "migrate:check": "blitz prisma migrate status",
    "migrate:compare": "git diff origin/main... -- db/migrations",
    "seed": "npm run predev && npm run migrate && blitz prisma migrate reset && blitz db seed",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "test": "vitest watch",
    "test:run": "vitest run --passWithNoTests",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "check": "npm run type-check && npm run lint && npm run format && npm run test:run",
    "updateStyles": "npx vite-node ./scripts/MapboxStyles/process.ts && npm run lint -- --fix && prettier \"src/components/MapInterface/mapData/topicsMapData/mapboxStyles/mapbox-layer-styles-by-group.json\" --write && github",
    "updateDatasets": "node ./datasets/process.cjs && node ./datasets/upload.cjs && node ./datasets/typesAndObject.cjs && prettier \"./src/components/MapInterface/mapData/sourcesMapData/datasets\" --write",
    "updateMasks": "npm run updateMasks:createGeojson && npm run updateMasks:createAndUploadMbtiles && github",
    "updateMasks:createGeojson": "npx vite-node ./scripts/RegionalMasks/createGeojson.ts && prettier \"scripts/RegionalMasks/geojson/\" --write",
    "updateMasks:createAndUploadMbtiles": "npx vite-node ./scripts/RegionalMasks/createMbtiles.ts && npm run updateDatasets",
    "updatePackages": "npx taze major --write",
    "release": "gh pr create --base main --head develop --title \"Release $(date '+%Y-%m-%d')\" --body \"\"",
    "prepare": "husky install"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.421.0",
    "@blitzjs/auth": "2.0.0-beta.33",
    "@blitzjs/next": "2.0.0-beta.33",
    "@blitzjs/rpc": "2.0.0-beta.33",
    "@headlessui/react": "^1.7.17",
    "@hookform/error-message": "2.0.1",
    "@hookform/resolvers": "3.3.1",
    "@mapbox/mapbox-gl-draw": "^1.4.3",
    "@popperjs/core": "^2.11.8",
    "@prisma/client": "5.3.1",
    "@sentry/nextjs": "7.72.0",
    "@turf/turf": "^6.5.0",
    "@types/mapbox__mapbox-gl-draw": "^1.4.3",
    "axios": "^1.5.1",
    "blitz": "2.0.0-beta.33",
    "clsx": "^2.0.0",
    "dompurify": "^3.0.5",
    "immer": "^10.0.2",
    "jsurl2": "^2.1.0",
    "maplibre-gl": "^3.3.1",
    "next": "13.5.3",
    "next-usequerystate": "1.8.1",
    "osm-auth": "^2.2.0",
    "pmtiles": "^2.10.0",
    "prisma": "5.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.46.2",
    "react-intl": "^6.4.7",
    "react-map-gl": "7.1.6",
    "react-popper": "^2.3.0",
    "react-remark": "^2.1.0",
    "secure-password": "4.0.0",
    "tiny-invariant": "^1.3.1",
    "zipson": "^0.2.12",
    "zod": "3.22.2",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@heroicons/react": "^2.0.18",
    "@next/bundle-analyzer": "13.5.3",
    "@next/env": "13.5.3",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "6.1.3",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@types/dompurify": "^3.0.3",
    "@types/jsurl2": "^2.1.2",
    "@types/node": "20.7.1",
    "@types/preview-email": "3.0.2",
    "@types/react": "18.2.23",
    "@types/react-dom": "^18.2.8",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@vitejs/plugin-react": "4.1.0",
    "@vitest/ui": "^0.34.5",
    "autoprefixer": "^10.4.16",
    "chalk": "^5.3.0",
    "eslint": "8.50.0",
    "eslint-config-next": "13.5.3",
    "eslint-config-prettier": "9.0.0",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "postcss": "^8.4.30",
    "prettier": "3.0.3",
    "prettier-plugin-prisma": "5.0.0",
    "prettier-plugin-tailwindcss": "0.5.4",
    "preview-email": "3.0.19",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.5"
  }
}
