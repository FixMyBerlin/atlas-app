{
  "name": "atlas-app",
  "version": "0.2.0",
  "author": "FixMyCity <hello@fixmycity.de>",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "start": "blitz start",
    "dev": "blitz dev --hostname 127.0.0.1 --port 5173",
    "predev": "if [[ $(docker inspect -f '{{.State.Running}}' atlas-db) = \"true\" ]]; then echo DOCKER: atlas-geo already running; else echo IMPORTANT: Start atlas-geo with npm run dev:up; fi",
    "build": "blitz build",
    "studio": "blitz prisma studio",
    "openTypes": "code \"node_modules/.prisma/client/index.d.ts\"",
    "migrate": "blitz prisma migrate dev",
    "migrate:create": "blitz prisma migrate dev --create-only && npm run migrate:create:openMigration",
    "migrate:create:openMigration": "code $(find ./db/migrations -type f -name \"migration.sql\" -cmin -1 | head -n 1)",
    "migrate:check": "blitz prisma migrate status",
    "migrate:compare": "git diff origin/main... -- db/migrations",
    "seed": "npm run predev && npm run migrate && blitz prisma migrate reset && blitz db seed",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "test": "vitest watch",
    "test:run": "vitest run --passWithNoTests",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "check": "npm run type-check && npm run lint && npm run format && npm run test:run",
    "updateStyles": "bun ./scripts/MapboxStyles/process.ts && npm run lint -- --fix && prettier \"src/core/components/MapInterface/mapData/topicsMapData/mapboxStyles/mapbox-layer-styles-by-group.json\" --write && github",
    "updateDatasets": "npm run updateDatasets:createPmtiles && npm run updateDatasets:upload && npm run updateDatasets:typesAndObject && prettier \"./src/core/components/MapInterface/mapData/sourcesMapData/datasets\" --write && github",
    "updateDatasets:createPmtiles": "bun ./datasets/createPmtiles.ts",
    "updateDatasets:upload": "bun ./datasets/upload.ts",
    "updateDatasets:typesAndObject": "bun ./datasets/typesAndObject.ts ",
    "updateMasks": "npm run updateMasks:createGeojson && npm run updateMasks:createPmtiles && npm run updateDatasets:upload && github",
    "updateMasks:createGeojson": "bun ./scripts/RegionalMasks/createGeojson.ts && prettier \"scripts/RegionalMasks/geojson/\" --write",
    "updateMasks:createPmtiles": "bun ./scripts/RegionalMasks/createPmtiles.ts",
    "updatePackages": "npx taze major --write",
    "release": "gh pr create --base main --head develop --title \"Release $(date '+%Y-%m-%d')\" --body \"\"",
    "prepare": "husky install"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "dependencies": {
    "@blitzjs/auth": "2.0.0-beta.34",
    "@blitzjs/next": "2.0.0-beta.34",
    "@blitzjs/rpc": "2.0.0-beta.34",
    "@headlessui/react": "^1.7.17",
    "@hookform/error-message": "2.0.1",
    "@hookform/resolvers": "2.9.11",
    "@mapbox/mapbox-gl-draw": "^1.4.3",
    "@popperjs/core": "^2.11.8",
    "@prisma/client": "5.5.2",
    "@turf/turf": "^6.5.0",
    "@types/mapbox__mapbox-gl-draw": "^1.4.5",
    "axios": "^1.6.0",
    "blitz": "2.0.0-beta.34",
    "clsx": "^1.2.1",
    "dompurify": "^3.0.6",
    "immer": "^9.0.21",
    "jsurl2": "^2.1.0",
    "maplibre-gl": "^2.4.0",
    "next": "13.5.6",
    "next-usequerystate": "1.9.1",
    "pmtiles": "^2.11.0",
    "prisma": "5.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "1.3.0",
    "react-hook-form": "7.47.0",
    "react-intl": "^6.5.1",
    "react-map-gl": "7.1.6",
    "react-popper": "^2.3.0",
    "react-remark": "^2.1.0",
    "secure-password": "4.0.0",
    "sharp": "0.32.6",
    "tiny-invariant": "^1.3.1",
    "zipson": "^0.2.12",
    "zod": "3.22.4",
    "zustand": "^4.4.4"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "3.438.0",
    "@heroicons/react": "^2.0.18",
    "@next/bundle-analyzer": "12.3.4",
    "@next/env": "13.5.6",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/dompurify": "^3.0.4",
    "@types/jsurl2": "^2.1.3",
    "@types/node": "18.18.7",
    "@types/preview-email": "2.0.1",
    "@types/react": "18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/web": "^0.0.117",
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@vitejs/plugin-react": "4.1.0",
    "@vitest/ui": "^0.28.5",
    "autoprefixer": "^10.4.16",
    "bun-types": "1.0.7",
    "chalk": "^5.3.0",
    "eslint": "8.52.0",
    "eslint-config-next": "13.5.6",
    "eslint-config-prettier": "8.10.0",
    "husky": "8.0.3",
    "jsdom": "20.0.3",
    "postcss": "^8.4.31",
    "prettier": "3.0.3",
    "prettier-plugin-prisma": "4.17.0",
    "prettier-plugin-tailwindcss": "0.5.6",
    "preview-email": "3.0.19",
    "tailwindcss": "^3.3.5",
    "typescript": "4.9.5",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6"
  }
}
