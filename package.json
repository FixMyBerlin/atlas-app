{
  "name": "atlas-app",
  "version": "0.2.0",
  "author": "FixMyCity <hello@fixmycity.de>",
  "license": "AGPL-3.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "scripts": {
    "start": "npx blitz@2.0.9 start --hostname 127.0.0.1 --port 3000",
    "dev": "npx blitz@2.0.9 dev --hostname 127.0.0.1 --port 5173",
    "predev": "if [ $(docker inspect -f '{{.State.Running}}' db) = \"true\" ]; then echo DOCKER: atlas-geo already running; else echo IMPORTANT: Start atlas-geo with npm run dev:up; fi && node scripts/npm-run-predev/checkMigrations.js",
    "build": "npx blitz@2.0.9 build",
    "bleach": "rm -rf node_modules/ && rm -rf .next/ && npm install",
    "bleach:full": "rm package-lock.json && npm run bleach",
    "studio": "npx blitz@2.0.9 prisma studio",
    "openTypes": "code \"node_modules/.prisma/client/index.d.ts\"",
    "migrate": "npx blitz@2.0.9 prisma migrate dev",
    "migrate:create": "npx blitz@2.0.9 prisma migrate dev --create-only && npm run migrate:create:openMigration",
    "migrate:create:openMigration": "code $(find ./db/migrations -type f -name \"migration.sql\" -cmin -1 | head -n 1)",
    "migrate:check": "npx blitz@2.0.9 prisma migrate status",
    "migrate:compare": "git diff origin/main... -- db/migrations",
    "seed": "npm run predev && npm run migrate && npx blitz@2.0.9 prisma migrate reset && npx blitz@2.0.9 db seed",
    "type-check": "tsc --noEmit",
    "lint": "next lint",
    "format": "prettier --ignore-path .gitignore \"(src|mailers)/**/*.+(ts|js|tsx|md)\" --write",
    "save-configs": "bun \"src/app/regionen/[regionSlug]/_hooks/useQueryState/useCategoriesConfig/v2/saveConfigs.ts\" && prettier --write \"src/**/v2/configs/index.ts\" \"src/**/v2/configs/*.ts\"",
    "test": "vitest watch",
    "test:run": "vitest run --passWithNoTests",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "check": "npm run type-check && npm run lint && npm run format && npm run test:run",
    "updateStyles": "bun ./scripts/MapboxStyles/process.ts && npm run lint -- --fix && prettier \"src/app/regionen/[regionSlug]/_mapData/mapDataSubcategories/mapboxStyles\" --write && github",
    "updateStaticDatasets": "bun ./scripts/StaticDatasets/updateStaticDatasets.ts",
    "updateStaticDatasets:dryRun": "bun ./scripts/StaticDatasets/updateStaticDatasets.ts --dry-run",
    "updateStaticDatasets:keepTmp": "bun ./scripts/StaticDatasets/updateStaticDatasets.ts --keep-tmp",
    "deleteAllStaticDatasets": "bun ./scripts/StaticDatasets/deleteAllStaticDatasets.ts",
    "updateMasks": "npm run updateMasks:createGeojson && npm run updateMasks:createPmtiles && npm run updateMasks:upload && github",
    "updateMasks:createGeojson": "bun ./scripts/RegionalMasks/createGeojson.ts && prettier \"scripts/RegionalMasks/geojson/\" --write",
    "updateMasks:createPmtiles": "bun ./scripts/RegionalMasks/createPmtiles.ts",
    "updateMasks:upload": "bun ./scripts/RegionalMasks/upload.ts",
    "updateTilesets": "bun ./scripts/MapboxTilesets/process.ts",
    "updatePackages:major": "npx taze major --write --includeLocked && npm install",
    "updatePackages:minor": "npx taze minor --write --includeLocked && npm install",
    "release": "gh pr create --base main --head develop --title \"Release $(date '+%Y-%m-%d')\" --body \"\"",
    "prepare": "husky",
    "postinstall": "patch-package",
    "link-atlas-static-data": "ln -s ../../../atlas-static-data/geojson scripts/StaticDatasets"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "dependencies": {
    "@blitzjs/auth": "2.0.9",
    "@blitzjs/next": "2.0.9",
    "@blitzjs/rpc": "2.0.9",
    "@headlessui/react": "2.1.1",
    "@hookform/error-message": "2.0.1",
    "@hookform/resolvers": "3.4.2",
    "@mapbox/mapbox-gl-draw": "^1.4.3",
    "@popperjs/core": "^2.11.8",
    "@prisma/client": "5.14.0",
    "@tanstack/react-query": "5.37.1",
    "@turf/turf": "^6.5.0",
    "@types/js-cookie": "3.0.6",
    "@types/lodash": "4.17.4",
    "@types/mapbox__mapbox-gl-draw": "^1.4.6",
    "adler-32": "1.3.1",
    "ajv": "8.12.0",
    "blitz": "2.0.9",
    "dompurify": "^3.1.4",
    "eslint-plugin-react-compiler": "0.0.0-experimental-0998c1e-20240625",
    "immer": "10.1.1",
    "js-cookie": "3.0.5",
    "jsts": "2.11.3",
    "jsurl2": "2.2.0",
    "lodash": "4.17.21",
    "maplibre-gl": "4.3.2",
    "next": "14.2.3",
    "next-auth": "4.24.7",
    "nuqs": "1.17.4",
    "pluralize": "8.0.0",
    "pmtiles": "3.0.5",
    "prisma": "5.14.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.51.5",
    "react-intl": "^6.6.8",
    "react-map-gl": "7.1.7",
    "react-popper": "^2.3.0",
    "react-remark": "^2.1.0",
    "secure-password": "4.0.0",
    "server-only": "0.0.1",
    "sharp": "0.32.6",
    "slugify": "1.6.6",
    "tailwind-merge": "2.3.0",
    "tiny-invariant": "^1.3.3",
    "use-resize-observer": "9.1.0",
    "zod": "3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "3.582.0",
    "@heroicons/react": "^2.1.3",
    "@next/bundle-analyzer": "12.3.4",
    "@next/env": "13.5.6",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@testing-library/jest-dom": "6.4.5",
    "@testing-library/react": "15.0.7",
    "@testing-library/react-hooks": "8.0.1",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/dompurify": "^3.0.5",
    "@types/jsurl2": "^2.1.4",
    "@types/node": "18.19.33",
    "@types/preview-email": "2.0.1",
    "@types/react": "18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/spritesmith": "3.4.5",
    "@types/web": "0.0.147",
    "@typescript-eslint/eslint-plugin": "7.10.0",
    "@vitejs/plugin-react": "4.3.0",
    "@vitest/ui": "1.6.0",
    "autoprefixer": "^10.4.19",
    "bun-types": "1.1.9",
    "chalk": "^5.3.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "husky": "9.0.11",
    "jsdom": "24.0.0",
    "minimatch": "9.0.4",
    "parse-gitignore": "2.0.0",
    "patch-package": "8.0.0",
    "postcss": "^8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-prisma": "5.0.0",
    "prettier-plugin-tailwindcss": "0.5.14",
    "preview-email": "3.0.19",
    "spritesmith": "3.4.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "use-changed-props": "0.1.0",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.6.0"
  }
}
