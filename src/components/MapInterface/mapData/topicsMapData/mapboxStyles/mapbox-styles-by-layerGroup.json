        "layout": {
          "text-size": [
            "interpolate",
            [
              "linear"
            ],
            [
              "zoom"
            ],
            10,
            10,
            22,
            13
          ],
          "text-font": [
            "Open Sans SemiBold",
            "Arial Unicode MS Regular"
          ],
          "icon-allow-overlap": true,
          "text-padding": 0,
          "text-offset": [
            0,
            1
          ],
          "icon-size": [
            "match",
            [
              "get",
              "population"
            ],
            [
              "6452"
            ],
            3,
            [
              "8945"
            ],
            4,
            [
              "10633",
              "11355"
            ],
            5,
            [
              "17017"
            ],
            8,
            [
              "38111"
            ],
            19,
            1
          ],
          "text-anchor": "top",
          "text-field": [
            "to-string",
            [
              "concat",
              [
                "get",
                "name"
              ],
              " \n ",
              [
                "get",
                "population"
              ]
            ]
          ],
          "icon-padding": 0,
          "text-max-width": 20
        },
        "paint": {
          "text-halo-color": "hsla(0, 6%, 97%, 0.91)",
          "text-halo-width": 1
        }
      }
    ]
  },
